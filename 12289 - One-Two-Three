#include <iostream>
    #include <algorithm>
    #include <cmath>
    #include <string>
    #include <iomanip> 
using namespace std;
int main()
{  long long z;  string x;
cin>>z;
for (int i=0;i<z;i++){
    cin>>x;
    if(x.size()>3)cout<<"3\n";
    else if((x[0]=='o'&&x[1]=='n')||(x[0]=='o'&&x[2]=='e')||(x[2]=='e'&&x[1]=='n'))cout<<"1\n";
    else cout<<"2\n";
}

    return 0;}
