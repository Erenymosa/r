#include <iostream>
    #include <algorithm>
    #include <cmath>
    #include <string>
    #include <iomanip> 
using namespace std;
int main()
{  string z,x;
cin>>z; cin>>x;

for(int i=0;i<z.size();i++){
    if(z[i]>='A'&&z[i]<='Z'){
      z[i]+=32;
      }
     if(x[i]>='A'&&x[i]<='Z'){
      x[i]+=32;
      }
}
if(z>x)cout<<"1";
else if(z<x)cout<<"-1";
else cout<<"0";
    return 0;}
