#include <iostream>
    #include <algorithm>
    #include <cmath>
    #include <string>
    #include <iomanip> 
using namespace std;
int main()
{long long z,a=0,m=0,r=0,g=0,i=0,t=0,c[6];    string x;
	cin>>z;
	for(int h=0;h<z;h++){ cin>>x;
		for(int j=0;j<x.size();j++){
		   if(x[j]=='A')a+=1;
		   else if(x[j]=='M')m+=1;
		   else if(x[j]=='R')r+=1;
		   else if(x[j]=='G')g+=1;
		   else if(x[j]=='I')i+=1;
		   else if(x[j]=='T')t+=1;
		   }
		   c[0]=m/1; 
		   c[1]=a/3;
		   c[2]=r/2;
		   c[3]=g/1;
		   c[4]=i/1;
		   c[5]=t/1;
		   sort(c,c+6);
		   cout<<c[0]<<endl;
		   a=0,m=0,r=0,g=0,i=0,t=0;}
return 0;
}
